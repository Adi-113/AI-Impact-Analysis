    "from sklearn.preprocessing import LabelEncoder\n",
    "selected_cols=['What is your age range?',\n",
    "    'What is your gender?',\n",
    "    'What is your education level?',\n",
    "    'What is your employment status?',\n",
    "    'knowledge_level_score', 'trust_ai_score',\"want_more_ai\",\"ai_impact_opinion\"\n",
    "]\n",
    "ai_data_filtered = ai_data[selected_cols].dropna()\n",
    "\n",
    "# Encode categorical columns\n",
    "X_encoded = ai_data_filtered.copy()\n",
    "for col in X_encoded.select_dtypes(include='object').columns:\n",
    "    X_encoded[col] = LabelEncoder().fit_transform(X_encoded[col])\n",
    "\n",
    "# Use this for clustering\n",
    "X_combined = X_encoded.values"
   ]
